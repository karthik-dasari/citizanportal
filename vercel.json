{
    "version": 2,
    "builds": [{
        "src": "CitizenSafety/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "64mb" , "runtime": "python3.9"}
    }],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "CitizenSafety/wsgi.py"
        }
    ]
}